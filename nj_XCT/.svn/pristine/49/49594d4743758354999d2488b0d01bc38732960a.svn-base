package com.xguanjia.hx.payroll.bean;

import java.io.Serializable;

/**
 * 
 * @author Administrator 每一个tab项的对应的工资
 */

public class EveryPayValueBean implements Serializable{

	private String useGroupingName = "";
	private String wages = "";
	private int useGroupingId;
	private String payRollId = "";
	public String getUseGroupingName() {
		return useGroupingName;
	}
	public void setUseGroupingName(String useGroupingName) {
		this.useGroupingName = useGroupingName;
	}
	public String getWages() {
		return wages;
	}
	public void setWages(String wages) {
		this.wages = wages;
	}
	public int getUseGroupingId() {
		return useGroupingId;
	}
	public void setUseGroupingId(int useGroupingId) {
		this.useGroupingId = useGroupingId;
	}
	public String getPayRollId() {
		return payRollId;
	}
	public void setPayRollId(String payRollId) {
		this.payRollId = payRollId;
	}
	
	

}
