<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
	<TextView
	    android:id="@+id/textViewVisitExecuteCustom"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginTop="5dp"
	    android:layout_marginLeft="10dp"
	    android:textSize="14sp"
	    android:textColor="@color/black_gray"/>
	
	<TextView
	    android:id="@+id/textViewVisitExecuteTarget"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginTop="4dp"
	    android:layout_marginLeft="10dp"
	    android:layout_below="@+id/textViewVisitExecuteCustom"
	    android:textSize="14sp"
	    android:textColor="@color/black_gray"/>
    
    <TextView
        android:id="@+id/textViewVisitExecuteContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewVisitExecuteTarget"
        android:layout_marginTop="4dp"
        android:layout_marginLeft="10dp"
        android:textColor="@color/black_gray"
        android:textSize="14sp"
        android:singleLine="true"
        android:typeface="monospace"/>

</RelativeLayout>
