<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
     >

    <ScrollView
        android:id="@+id/sv_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TableLayout
                android:id="@+id/tableLayout01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/radioInfo"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="5dip"
                android:layout_marginLeft="5dip"
                android:layout_marginRight="5dip"
                android:layout_marginTop="5dp"
                android:background="@drawable/gray_form_frame_bg"
                android:stretchColumns="1"
                android:visibility="visible" >

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="姓名：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsName"
                        style="@style/TableRowEdit"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="职位：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsPosition"
                        style="@style/TableRowEdit"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="性别：" >
                    </TextView>

                    <RadioGroup
                        android:id="@+id/radioGroupContactsSex"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:orientation="horizontal" >

                        <RadioButton
                            android:id="@+id/radioButtonBoy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:checked="true"
                            android:text="男"
                            android:textColor="@color/black_gray"
                            android:textSize="18sp" />

                        <RadioButton
                            android:id="@+id/radioButtonGirl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:text="女"
                            android:textColor="@color/black_gray"
                            android:textSize="18sp" />
                    </RadioGroup>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="年龄：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsAge"
                        style="@style/TableRowEdit"
                        android:numeric="integer"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="手机号码：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsMobile"
                        style="@style/TableRowEdit"
                        android:phoneNumber="true"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:phoneNumber="true"
                        android:text="座机号码：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsPhone"
                        style="@style/TableRowEdit"
                        android:phoneNumber="true"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="Q	Q：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsQQ"
                        style="@style/TableRowEdit"
                        android:numeric="integer"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="Email：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsEmail"
                        style="@style/TableRowEdit"
                        android:digits="@string/register_name_digits"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="地址：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsAddress"
                        style="@style/TableRowEdit"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="备注：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsContent"
                        style="@style/TableRowEdit"
                        android:text="" >
                    </EditText>
                </TableRow>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="2.0dip"
                    android:background="@drawable/crm_divider_brokenline" />

                <TableRow
                    android:layout_marginBottom="5dip"
                    android:layout_marginTop="5dip" >

                    <TextView
                        style="@style/TableRowText"
                        android:text="传真：" >
                    </TextView>

                    <EditText
                        android:id="@+id/editTextContactsFax"
                        style="@style/TableRowEdit"
                        android:text="" >
                    </EditText>

                    <TableRow
                        android:layout_marginBottom="5dip"
                        android:layout_marginTop="5dip" >

                        <TextView
                            style="@style/TableRowText"
                            android:text="生日：" >
                        </TextView>

                        <EditText
                            android:id="@+id/editTextContactsBirthday"
                            style="@style/TableRowEdit"
                            android:text="" >
                        </EditText>
                    </TableRow>
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>